{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "None",
      "Microsoft.AspNetCore": "Warning",
      "MassTransit": "Debug",
      "MassTransit.MongoDbIntegration": "Debug",
      "MongoDb": "Debug"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss"
      }
    }
  },
  "MANAGER_HOSTNAME": "manager",
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "mongodb://mongo1,mongo2,mongo3"
  }
}
