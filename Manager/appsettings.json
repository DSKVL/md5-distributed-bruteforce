{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MassTransit": "Debug",
      "MongoDb": "Debug"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss"
      }
    }
  },
  "AllowedHosts": "*",
  "Alphabet": "1234567890abcdefghijklmnopqrstuvwxyz",
  "MaxLength": "10",
  "Timeout": "1000000",
  "WORKER_HOSTNAME_PREFIX": "ww",
  "WORKER_COUNT": "1",
  "ConnectionStrings": {
    "Default": "mongodb://mongo1,mongo2,mongo3"
  }
}
