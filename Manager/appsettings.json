{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "FormatterName": "Simple",
    "FormatterOptions": {
      "SingleLine": true
    }
  },
  "AllowedHosts": "*",
  "Alphabet": "1234567890abcdefghijklmnopqrstuvwxyz",
  "MaxLength": "10",
  "Timeout": "10000",
  "WORKER_HOSTNAME_PREFIX": "ww",
  "WORKER_COUNT": "1"
}
